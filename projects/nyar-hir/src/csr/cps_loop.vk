def test_loop1(i) {
	loop {
	    if (i > 0) {
	        i -= 1
	        continue
	    }
	    else {
	        break
	    }
	}
	i + 1
}

class __csr_test_loop1 {
	i,
}

extends __csr_test_loop1 {
	__k1() {
	    i + 1
	}
	__k2() {
	    if (i > 0) {
	        i -= 1
	        self.__k2() // continue
	    }
	    else {
	        self.__k1() // break
	    }
	}
	__main(i) {
	    i = 0
	    self.__k2()
	}
}
